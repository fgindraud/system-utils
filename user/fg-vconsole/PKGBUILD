pkgname='fg-vconsole'
pkgver=2
pkgrel=1
pkgdesc='Font and keymap for TTY'
arch=('any')
license=('MIT')
depends=('terminus-font')
source=('fr-custom-oss.map.gz')
noextract=('fr-custom-oss.map.gz')
md5sums=('fa3116991e8c04ab3ce824db72d0103f')

package() {
	install -D -m644 'fr-custom-oss.map.gz' "${pkgdir}/usr/share/kbd/keymaps/i386/azerty/fr-custom-oss.map.gz"
}
